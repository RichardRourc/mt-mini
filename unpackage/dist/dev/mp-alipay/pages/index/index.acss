
.food-type{
    position:relative;
    display:-webkit-box;
    display:-webkit-flex;
    display:-ms-flexbox;
    display:flex;
    -webkit-flex-wrap:wrap;
        -ms-flex-wrap:wrap;
            flex-wrap:wrap;
    -webkit-box-align:center;
    -webkit-align-items:center;
        -ms-flex-align:center;
            align-items:center;
    width:98%;
    height:300rpx;
    padding:38rpx 0rpx;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
        -ms-flex-pack: center;
            justify-content: center;
}
.food-type-list{
    display:inline-block;
    width:20%;
    height:120rpx;
    text-align:center;
    position:relative;
    margin-bottom:16rpx;
}
.food-type-list text{
    position:relative;
    /* left:-15upx; */
}
.food-type-list-image{
    display:inline-block;
    width:96rpx;
    height:54rpx;
    background-color: skyblue;
}


.recommend{
	position:relative;
	width:96%;
	margin:auto;
}
.recommend .recommend-head .title{
	font-size: 42rpx;
	font-weight: 600;
}
.recommend .recommend-head .food-recommend__title__more{
	position:absolute;
	top:5rpx;
	right:0rpx;
	font-size:24rpx;
	color:#666;
}
.recommend .recommend-head .more .dayu{
	top:0rpx;
}
.food-ul{
	/* border:1px solid red; */
	padding:12rpx 0rpx; 
	/* width:400%; */
	white-space: nowrap;
}
.food-ul-li{
	display:inline-block;
	padding-right:12rpx;
}
.food-ul-li .food{
	position:relative;
}
.food-ul-li .food image{
	position:absolute;
}
.food-ul-li .food .food-image{
	position:relative;
	width:300rpx;
	height:180rpx;
	border-radius:12rpx;
}
.food-ul-li .food .image--wrap{
	position:absolute;
	bottom:0rpx;
}
.food-ul-li .food .shop{
	/* position:relative; */
	bottom:0rpx;
	left:12rpx;
	width:80rpx;
	height:40rpx;
}
.food + view{
	padding:8rpx;
}
.reason{
	font-size:32rpx;
	color:#ccc;
}
.food-recommend__more{
	display:inline-block;
	height:164rpx;
	width:24rpx;
	padding:8rpx 12rpx;
	color:#ccc;
	border:1px solid #ccc;
	white-space:normal;
	font-size:32rpx;
	color:#ccc;
	vertical-align:top;
	border-radius:12rpx;
}


.banner{
	width:96%;
	margin:auto;
	position:relative;
	padding:24rpx 0rpx;
}
.banner > ._img{
	height:180rpx;
	width:100%;
	border-radius:24rpx;
}
.banner .banner__title{
	position:absolute;
	padding:6rpx 8rpx;
	bottom:30rpx;
	right:-8rpx;
	font-size:12rpx;
	color:white;
	background-color: #666;
	opacity:.5;
	border-radius:4rpx;
}

